<tr>
  <td><%= view.id %></td>
  <td><%= view.name %></td>
  <td>
    <%= link_to t(".edit"), edit_admin_view_path(view), remote: true, class: "btn btn-block btn-primary btn-action" %>
  </td>
  <td>
    <%= link_to t(".delete"), admin_view_path(view), method: :delete, data: {confirm: "You sure?"}, class: "btn btn-block btn-danger btn-action" %>
  </td>
</tr>
